pages = @presenter.lookup

json.object! do
  json.total_page pages.size
  json.pages do
    json.object! do
      pages.each do |page|
        json.set! page.page_number do
          json.object! do
            json.from page.first_verse_key
            json.to page.last_verse_key
          end
        end
      end
    end
  end
end
